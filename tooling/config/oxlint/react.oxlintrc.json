{
	"$schema": "./node_modules/oxlint/configuration_schema.json",
	"extends": ["./base.oxlintrc.json"],
	"plugins": ["react", "jsx-a11y"],
	"env": {
		"browser": true
	},
	"rules": {
		"react/jsx-no-leaked-render": "error",
		"react-hooks/exhaustive-deps": "warn",
		"jsx-a11y/alt-text": "error"
	}
}
