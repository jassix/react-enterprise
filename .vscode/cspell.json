{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "import": [],
  "ignorePaths": [".turbo", ".vscode", "dist", "node_modules"],
  "dictionaries": ["Dev", "Namings", "Other"],
  "dictionaryDefinitions": [
    { "name": "Dev", "path": "cspell/dev.txt", "noSuggest": true },
    { "name": "Namings", "path": "cspell/namings.txt", "noSuggest": true },
    { "name": "Other", "path": "cspell/other.txt", "noSuggest": true }
  ]
}